
if (keyboard_check(vk_up) && !place_meeting(x,y-1,obj_collider)) {
    y -= 2;

}
if (keyboard_check(vk_left) && !place_meeting(x-1,y,obj_collider)) {
    x -= 2;

}
if (keyboard_check(vk_down) && !place_meeting(x,y+1,obj_collider)) {
    y += 2;

}
if (keyboard_check(vk_right) && !place_meeting(x+1,y,obj_collider)) {
    x += 2;

}
